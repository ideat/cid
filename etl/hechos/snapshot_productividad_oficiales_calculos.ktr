    <sql>select b.fecha_key, a.oficial_key, b.salario, b.porcentaje_incentivo_desembolso,&#xd;&#xa;case&#xd;&#xa;  when b.meta_desembolso_minima &#x3c;&#x3d; &#x28;a.desembolsado&#x2a;a.tipo_cambio_me&#x29; then b.salario &#x2a; b.porcentaje_incentivo_desembolso&#xd;&#xa;  else 0&#xd;&#xa;end as monto_incentivo_desembolso,&#xd;&#xa;&#x28;a.saldo_mora &#x2f; a.saldo_me&#x29;&#x2a;100 as porcentaje_mora,&#xd;&#xa;b.incentivo_mora1,&#xd;&#xa;case&#xd;&#xa;  when  &#x28;a.saldo_mora &#x2f; a.saldo_me&#x29; &#x3c; b.porcentaje_mora2&#xd;&#xa;      and &#x28;a.saldo_mora &#x2f; a.saldo_me&#x29;&#x3c;&#x3d; b.porcentaje_mora1 then b.salario &#x2a; b.incentivo_mora1&#xd;&#xa;  else 0&#xd;&#xa;end as monto_incentivo_mora1,&#xd;&#xa;b.incentivo_mora2,&#xd;&#xa;case&#xd;&#xa;  when  &#x28;a.saldo_mora &#x2f; a.saldo_me&#x29; &#x3c; b.porcentaje_mora3&#xd;&#xa;      and &#x28;a.saldo_mora &#x2f; a.saldo_me&#x29;&#x3c;&#x3d; b.porcentaje_mora2 then b.salario &#x2a; b.incentivo_mora2&#xd;&#xa;  else 0&#xd;&#xa;end as monto_incentivo_mora2,&#xd;&#xa;b.incentivo_mora3,&#xd;&#xa;case&#xd;&#xa;  when  &#x28;a.saldo_mora &#x2f; a.saldo_me&#x29;&#x3c;&#x3d; b.porcentaje_mora3 then b.salario &#x2a; b.incentivo_mora3&#xd;&#xa;  else 0&#xd;&#xa;end as monto_incentivo_mora3,&#xd;&#xa;a.saldo_mora,&#xd;&#xa;a.recuperado,&#xd;&#xa;a.desembolsado,&#xd;&#xa;a.monto_castigado,&#xd;&#xa;a.saldo_me &#x2a; a.tipo_cambio_me as total_cartera_oficial&#xd;&#xa;&#xd;&#xa;from &#xd;&#xa;&#x28;select p.oficial_key,  sum&#x28;s.monto_desembolsado_me&#x29; as desembolsado, sum&#x28;s.saldo_me&#x29; as saldo_me,&#xd;&#xa;sum&#x28;saldo_castigado_me&#x29; as monto_castigado, sum&#x28;s.monto_recuperado_me&#x29; as recuperado,&#xd;&#xa;sum &#x28;&#xd;&#xa; CASE&#xd;&#xa;   when s.dias_mora &#x3e; 30 then s.saldo_me&#xd;&#xa;   else 0&#xd;&#xa; end&#xd;&#xa;&#x29; as saldo_mora,&#xd;&#xa;avg&#x28;s.tipo_cambio_me&#x29; as tipo_cambio_me&#xd;&#xa;from snapshot_credito_fact s&#xd;&#xa;inner join snapshot_parametros_productividad_oficial_fact p on &#x28;s.oficial_key &#x3d; p.oficial_key&#x29;&#xd;&#xa;inner join oficial_dim o on &#x28;o.oficial_key &#x3d; p.oficial_key&#x29;&#xd;&#xa;inner join fecha_dim f on &#x28;f.fecha_key &#x3d; p.fecha_key&#x29;&#xd;&#xa;where&#xd;&#xa; s.fecha_key &#x3d; to_date&#x28;&#x27;30-06-2014&#x27;,&#x27;dd-MM-yyyy&#x27;&#x29;&#xd;&#xa;group by 1&#x29; a inner join &#xd;&#xa;&#x28;select p.fecha_key, p.oficial_key, p.meta_desembolso_maxima,&#xd;&#xa;p.salario,&#xd;&#xa;p.meta_desembolso_minima, p.incentivo_mora1, p.incentivo_mora2, p.incentivo_mora3,&#xd;&#xa;p.porcentaje_incentivo_desembolso, p.porcentaje_mora1, p.porcentaje_mora2,&#xd;&#xa;p.porcentaje_mora3&#xd;&#xa;from snapshot_parametros_productividad_oficial_fact p &#xd;&#xa;where p.fecha_key &#x3d; to_date&#x28;&#x27;30-06-2014&#x27;,&#x27;dd-MM-yyyy&#x27;&#x29;&#x29; b on &#x28;a.oficial_key &#x3d; b.oficial_key&#x29;</sql>
