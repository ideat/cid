        <class_source><![CDATA[import java.math.BigDecimal;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    row = createOutputRow(row, data.outputRowMeta.size());

	Long prmprstat = 0L;
	String prmprstatNombre = get(Fields.In, "prmprstat_nombre").getString(row);

	if (prmprstatNombre.startsWith("VIGENTE CORTO PLAZO")) {
		prmprstat = 2L;
	}
	if (prmprstatNombre.startsWith("CANCELADO"))  {
		prmprstat = 9L;
	}
	if (prmprstatNombre.startsWith("CASTIGADO"))  {
		prmprstat = 7L;
	}
	if (prmprstatNombre.startsWith("VENCIDO"))  {
		prmprstat = 5L;
	}
	if (prmprstatNombre.startsWith("EJECUCION"))  {
		prmprstat = 6L;
	}
	

	get(Fields.Out, "prmprstat").setValue(row, prmprstat);
			

	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
]]></class_source>
