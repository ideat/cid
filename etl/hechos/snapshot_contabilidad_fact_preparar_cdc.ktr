        <class_source><![CDATA[import java.util.Date;
import org.joda.time.DateTime;


public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    row = createOutputRow(row, data.outputRowMeta.size());

	DateTime max_fecha_dw_original = new DateTime(get(Fields.In, "max_fecha_dw").getDate(row));

	//Retrasar la ultima fecha de carga, para volver a cargar datos
	DateTime max_fecha_dw = new DateTime(get(Fields.In, "max_fecha_dw").getDate(row)).minusDays(15);

    get(Fields.Out, "max_fecha_dw").setValue(row, max_fecha_dw.toDate());	
    get(Fields.Out, "max_fecha_dw_original").setValue(row, max_fecha_dw_original.toDate());	
		
 
	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
]]></class_source>
