        <class_source><![CDATA[import java.util.Arrays;
import java.math.BigDecimal;
import java.util.Date;
import java.text.SimpleDateFormat;
import java.text.ParseException;
import java.util.Calendar;
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
//    final long MILE_SEGUNDOS_DIAS = 24 * 60 * 60 * 1000;

    row = createOutputRow(row, data.outputRowMeta.size());

   Double total_sus_aux = 0.0;
   Double ajuste = 0.0;
   Double total_sus_rep =0.0;
   Double total_bs_rep = 0.0;
   Double banda_30 = 0.0;
   Double banda_60 = 0.0;
   Double banda_90 = 0.0;
   Double banda_180 = 0.0;
   Double banda_360 = 0.0;
   Double banda_720 = 0.0;
   Double banda_721 = 0.0;
   Double banda_30_bs = 0.0;
   Double banda_60_bs = 0.0;
   Double banda_90_bs = 0.0;
   Double banda_180_bs = 0.0;
   Double banda_360_bs = 0.0;
   Double banda_720_bs = 0.0;
   Double banda_721_bs = 0.0;
   Double banda_30_sus = 0.0;
   Double banda_60_sus = 0.0;
   Double banda_90_sus = 0.0;
   Double banda_180_sus = 0.0;
   Double banda_360_sus = 0.0;
   Double banda_720_sus = 0.0;
   Double banda_721_sus = 0.0;
   Double total = 0.0;
   Double total_bs = 0.0;
   Double total_sus = 0.0;
  // Double saldo_credito_me = 0.0;

   Double saldo_bs_exp_sus_30 =  get(Fields.In, "saldo_bs_exp_sus_30").getNumber(row);
   Double saldo_sus_exp_sus_30 =  get(Fields.In, "saldo_sus_exp_sus_30").getNumber(row);

   Double saldo_bs_exp_sus_60 =  get(Fields.In, "saldo_bs_exp_sus_60").getNumber(row);
   Double saldo_exp_sus_60 =  get(Fields.In, "saldo_exp_sus_60").getNumber(row);

   Double saldo_bs_exp_sus_90 =  get(Fields.In, "saldo_bs_exp_sus_90").getNumber(row);
   Double saldo_exp_sus_90 =  get(Fields.In, "saldo_exp_sus_90").getNumber(row);

   Double saldo_bs_exp_sus_180 =  get(Fields.In, "saldo_bs_exp_sus_180").getNumber(row);
   Double saldo_exp_sus_180 =  get(Fields.In, "saldo_exp_sus_180").getNumber(row);

   Double saldo_bs_exp_sus_360 =  get(Fields.In, "saldo_bs_exp_sus_360").getNumber(row);
   Double saldo_exp_sus_360 =  get(Fields.In, "saldo_exp_sus_360").getNumber(row);

   Double saldo_bs_exp_sus_720 =  get(Fields.In, "saldo_bs_exp_sus_720").getNumber(row);
   Double saldo_exp_sus_720 =  get(Fields.In, "saldo_exp_sus_720").getNumber(row);

   Double saldo_bs_exp_sus_mas_720 =  get(Fields.In, "saldo_bs_exp_sus_mas_720").getNumber(row);
   Double saldo_exp_sus_mas_720 =  get(Fields.In, "saldo_exp_sus_mas_720").getNumber(row);
   Double tipo_cambio = get(Fields.In, "tipo_cambio").getNumber(row);

   Double saldo_credito_me =  get(Fields.In, "saldo_credito_me").getNumber(row);
   
   //BigDecimal saldo_credito_med = get(Fields.In, "saldo_credito_me").getBigNumber(row);
   
  // saldo_credito_me = saldo_credito_med.doubleValue();
   
  banda_30 = (saldo_bs_exp_sus_30 + saldo_sus_exp_sus_30)*tipo_cambio;
  banda_60 = (saldo_bs_exp_sus_60 + saldo_exp_sus_60)*tipo_cambio;
  banda_90 = (saldo_bs_exp_sus_90 + saldo_exp_sus_90)*tipo_cambio;
  banda_180 = (saldo_bs_exp_sus_180 + saldo_exp_sus_180)*tipo_cambio;
  banda_360 = (saldo_bs_exp_sus_360 + saldo_exp_sus_360)*tipo_cambio;
  banda_720 = (saldo_bs_exp_sus_720 + saldo_exp_sus_720)*tipo_cambio;
  banda_721 = (saldo_bs_exp_sus_mas_720 + saldo_exp_sus_mas_720)*tipo_cambio; 


//  logBasic("*** saldo_bs_exp_sus_30 *** > " + saldo_bs_exp_sus_30);
  banda_30_bs = (saldo_bs_exp_sus_30)*tipo_cambio;
//  logBasic("*** banda_30_bs *** > " + banda_30_bs);
  banda_60_bs = (saldo_bs_exp_sus_60)*tipo_cambio;
  banda_90_bs = (saldo_bs_exp_sus_90)*tipo_cambio;
  banda_180_bs = (saldo_bs_exp_sus_180)*tipo_cambio;
  banda_360_bs = (saldo_bs_exp_sus_360)*tipo_cambio;
  banda_720_bs = (saldo_bs_exp_sus_720)*tipo_cambio;
  banda_721_bs = (saldo_bs_exp_sus_mas_720)*tipo_cambio; 

