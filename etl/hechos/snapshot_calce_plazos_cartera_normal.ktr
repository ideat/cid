  banda_90_sus = (saldo_exp_sus_90)*tipo_cambio;
  banda_180_sus = (saldo_exp_sus_180)*tipo_cambio;
  banda_360_sus = (saldo_exp_sus_360)*tipo_cambio;
  banda_720_sus = (saldo_exp_sus_720)*tipo_cambio;
  banda_721_sus = (saldo_exp_sus_mas_720)*tipo_cambio; 


