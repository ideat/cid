             + saldo_bs_exp_sus_360 + saldo_exp_sus_360 + saldo_bs_exp_sus_720 + saldo_exp_sus_720 
			 + saldo_bs_exp_sus_mas_720 + saldo_exp_sus_mas_720;


   ajuste = saldo_credito_me.doubleValue() - total_sus;
//   logBasic("*** saldo_credito_me *** > " + saldo_credito_me);
//    logBasic("*** ajuste *** > " + ajuste);
   total = total_sus_aux *tipo_cambio;
   
   Double saldo_bs_exp_sus_mas_720_rev = 0.0;
   Double saldo_exp_sus_mas_720_rev = 0.0;

   saldo_bs_exp_sus_mas_720_rev = saldo_bs_exp_sus_mas_720 + (ajuste/2);
   saldo_exp_sus_mas_720_rev = saldo_exp_sus_mas_720 + (ajuste/2);
   
   total_sus_rep = saldo_sus_exp_sus_30 +  saldo_exp_sus_60
             + saldo_exp_sus_90 + saldo_exp_sus_180
             + saldo_exp_sus_360 + saldo_exp_sus_720 
			 + saldo_exp_sus_mas_720_rev;

  total_sus = total_sus_rep * tipo_cambio;

   total_bs_rep = saldo_bs_exp_sus_30 +  saldo_bs_exp_sus_60 
             + saldo_bs_exp_sus_90 + saldo_bs_exp_sus_180
             + saldo_bs_exp_sus_360 + saldo_bs_exp_sus_720 
			 + saldo_bs_exp_sus_mas_720_rev;

  total_bs = total_bs_rep * tipo_cambio;
        

//   get(Fields.Out, "saldo_bs_exp_sus_30").setValue(row, saldo_bs_exp_sus_30);
//   get(Fields.Out, "saldo_sus_exp_sus_30").setValue(row,saldo_sus_exp_sus_30);	

//   get(Fields.Out, "saldo_bs_exp_sus_60").setValue(row,saldo_bs_exp_sus_60);
//   get(Fields.Out, "saldo_exp_sus_60").setValue(row, saldo_exp_sus_60);

//   get(Fields.Out, "saldo_bs_exp_sus_90").setValue(row,saldo_bs_exp_sus_90);
//   get(Fields.Out, "saldo_exp_sus_90").setValue(row, saldo_exp_sus_90);
 

//   get(Fields.Out, "saldo_bs_exp_sus_180").setValue(row,saldo_bs_exp_sus_180);
//   get(Fields.Out, "saldo_exp_sus_180").setValue(row, saldo_exp_sus_180);
//   get(Fields.Out, "saldo_monto_180_bs").setValue(row, saldo_monto_180_bs);

//   get(Fields.Out, "saldo_bs_exp_sus_360").setValue(row,saldo_bs_exp_sus_360);
//   get(Fields.Out, "saldo_exp_sus_360").setValue(row, saldo_exp_sus_360);

//   get(Fields.Out, "saldo_bs_exp_sus_720").setValue(row,saldo_bs_exp_sus_720);
//   get(Fields.Out, "saldo_exp_sus_720").setValue(row, saldo_exp_sus_720);


   get(Fields.Out, "saldo_bs_exp_sus_mas_720_rev").setValue(row,saldo_bs_exp_sus_mas_720_rev);
   get(Fields.Out, "saldo_exp_sus_mas_720_rev").setValue(row, saldo_exp_sus_mas_720_rev);
   get(Fields.Out, "total_sus_rep").setValue(row,total_sus_rep);
   get(Fields.Out, "total_bs_rep").setValue(row, total_bs_rep);
  // get(Fields.Out, "total_sus").setValue(row, total_sus);
   get(Fields.Out, "saldo_credito_me").setValue(row, saldo_credito_me);
   
   get(Fields.Out, "banda_30").setValue(row, banda_30);  
   get(Fields.Out, "banda_60").setValue(row, banda_60);
   get(Fields.Out, "banda_90").setValue(row, banda_90);
   get(Fields.Out, "banda_180").setValue(row, banda_180);
   get(Fields.Out, "banda_360").setValue(row, banda_360);
   get(Fields.Out, "banda_720").setValue(row, banda_720);
   get(Fields.Out, "banda_721").setValue(row, banda_721); 
   get(Fields.Out, "total").setValue(row, total);

   get(Fields.Out, "banda_30_bs").setValue(row, banda_30_bs);  
   get(Fields.Out, "banda_60_bs").setValue(row, banda_60_bs);
   get(Fields.Out, "banda_90_bs").setValue(row, banda_90_bs);
   get(Fields.Out, "banda_180_bs").setValue(row, banda_180_bs);
   get(Fields.Out, "banda_360_bs").setValue(row, banda_360_bs);
   get(Fields.Out, "banda_720_bs").setValue(row, banda_720_bs);
   get(Fields.Out, "banda_721_bs").setValue(row, banda_721_bs); 
   get(Fields.Out, "total_bs").setValue(row, total_bs);

   get(Fields.Out, "banda_30_sus").setValue(row, banda_30_sus);  
   get(Fields.Out, "banda_60_sus").setValue(row, banda_60_sus);
   get(Fields.Out, "banda_90_sus").setValue(row, banda_90_sus);
   get(Fields.Out, "banda_180_sus").setValue(row, banda_180_sus);
   get(Fields.Out, "banda_360_sus").setValue(row, banda_360_sus);
   get(Fields.Out, "banda_720_sus").setValue(row, banda_720_sus);
   get(Fields.Out, "banda_721_sus").setValue(row, banda_721_sus); 
   get(Fields.Out, "total_sus").setValue(row, total_sus);

   
	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;

}
