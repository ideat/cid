    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.math.BigDecimal;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    row = createOutputRow(row, data.outputRowMeta.size());

	double saldoCastigado = get(Fields.In, "hubo_castigo").getInteger(row) > 0 ? get(Fields.In, "saldo").getNumber(row).doubleValue() : 0;

	Long huboDesembolso = get(Fields.In, "monto_desembolsado").getBigNumber(row).doubleValue() > 0 ? 1L : 0L;
	Long huboRecuperacion = get(Fields.In, "monto_recuperado").getBigNumber(row).doubleValue() > 0 ? 1L : 0L;
	Long huboIntereses = get(Fields.In, "monto_intereses").getBigNumber(row).doubleValue() > 0 ? 1L : 0L;
    

	Long idTasaExterna = get(Fields.In, "l_tsex").getInteger(row);
	Double tasaVariable = get(Fields.In, "l_spread").getNumber(row) 
							+ (idTasaExterna == null ? get(Fields.In, "l_tbase").getNumber(row) : get(Fields.In, "prtextasa").getNumber(row));


//	logBasic("*** prmprcmon *** > " + get(Fields.In, "prmprcmon").getInteger(row));
//	logBasic("*** tipo_cambio_me *** > " + get(Fields.In, "tipo_cambio_me").getNumber(row).doubleValue());
//	logBasic("*** prmprmdes *** > " + get(Fields.In, "prmprmdes").getNumber(row).doubleValue());

	Double saldoME = get(Fields.In, "prmprcmon").getInteger(row) == 1L ? get(Fields.In, "saldo").getNumber(row).doubleValue() / get(Fields.In, "tipo_cambio_me").getNumber(row).doubleValue()  
																						: get(Fields.In, "saldo").getNumber(row).doubleValue();
	Double saldoCastigadoME = get(Fields.In, "prmprcmon").getInteger(row) == 1L ? saldoCastigado / get(Fields.In, "tipo_cambio_me").getNumber(row).doubleValue()
																					: saldoCastigado;

	Double montoDesembolsadoME = get(Fields.In, "prmprcmon").getInteger(row) == 1L ? get(Fields.In, "monto_desembolsado").getNumber(row).doubleValue() / get(Fields.In, "tipo_cambio_me").getNumber(row).doubleValue()  
																						: get(Fields.In, "monto_desembolsado").getNumber(row).doubleValue();
  	
	Double montoRecuperadoME = get(Fields.In, "prmprcmon").getInteger(row) == 1L ? get(Fields.In, "monto_recuperado").getNumber(row).doubleValue() / get(Fields.In, "tipo_cambio_me").getNumber(row).doubleValue()  
																						: get(Fields.In, "monto_recuperado").getNumber(row).doubleValue();
  	
	Double montoInteresesME = get(Fields.In, "prmprcmon").getInteger(row) == 1L ? get(Fields.In, "monto_intereses").getNumber(row).doubleValue() / get(Fields.In, "tipo_cambio_me").getNumber(row).doubleValue()  
																						: get(Fields.In, "monto_intereses").getNumber(row).doubleValue();

	Double montoTotalDesembolsadoME = get(Fields.In, "prmprcmon").getInteger(row) == 1L ? get(Fields.In, "prmprmdes").getNumber(row).doubleValue() / get(Fields.In, "tipo_cambio_me").getNumber(row).doubleValue()  
																						: get(Fields.In, "prmprmdes").getNumber(row).doubleValue();

	Long esClienteNuevo = get(Fields.In, "nro_creditos").getInteger(row) == 1L ? 1L : 0L;

    get(Fields.Out, "hubo_desembolso").setValue(row, huboDesembolso);	
    get(Fields.Out, "hubo_recuperacion").setValue(row, huboRecuperacion);	
    get(Fields.Out, "hubo_intereses").setValue(row, huboIntereses);
	get(Fields.Out, "tasa_variable").setValue(row, tasaVariable);
	get(Fields.Out, "saldo_me").setValue(row,new BigDecimal(saldoME));
	get(Fields.Out, "saldo_castigado").setValue(row,new BigDecimal(saldoCastigado));
	get(Fields.Out, "saldo_castigado_me").setValue(row,new BigDecimal(saldoCastigadoME));
	get(Fields.Out, "monto_desembolsado_me").setValue(row,new BigDecimal(montoDesembolsadoME));
	get(Fields.Out, "monto_recuperado_me").setValue(row,new BigDecimal(montoRecuperadoME));
	get(Fields.Out, "monto_intereses_me").setValue(row,new BigDecimal(montoInteresesME));
	get(Fields.Out, "monto_total_desembolsado_me").setValue(row,new BigDecimal(montoTotalDesembolsadoME));
	get(Fields.Out, "es_cliente_nuevo").setValue(row, esClienteNuevo);
			

	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
]]></class_source>
