        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.util.Date;
import org.joda.time.DateTime;

//Usa las columnas nombre_cuenta_X como variables auxiliares para derivar los codigo de cuenta en todos los niveles
public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    row = createOutputRow(row, data.outputRowMeta.size());

	Date fecha_prevision = get(Fields.In, "fecha_key").getDate(row);
	DateTime dateTime = new DateTime(fecha_prevision);


	Date fecha_saldo = dateTime.dayOfMonth().withMaximumValue().toDate();


    get(Fields.Out, "fecha_saldo").setValue(row, fecha_saldo);	
		
 
	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
]]></class_source>
