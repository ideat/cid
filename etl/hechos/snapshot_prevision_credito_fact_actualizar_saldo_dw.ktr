    <sql>SELECT   &#xd;&#xa;f.credito_key&#xd;&#xa;, f.fecha_key&#xd;&#xa;, f.agencia_key&#xd;&#xa;, f.moneda_key&#xd;&#xa;, f.oficial_key&#xd;&#xa;, f.linea_credito_key&#xd;&#xa;, f.saldo_credito_mn&#xd;&#xa;, c.codigo_tx&#xd;&#xa;FROM snapshot_prevision_credito_fact f&#xd;&#xa;INNER JOIN credito_dim c ON f.credito_key &#x3d; c.credito_key &#xd;&#xa;WHERE &#xd;&#xa;--fecha_key &#x3e; to_date&#x28;&#x27;&#x24;&#x7b;MIN_FECHA_EXTRACCION_PREVISION&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;f.fecha_key &#x3e; to_date&#x28;&#x27;2008-12-31&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;&#xd;&#xa;-- Para depuracion&#xd;&#xa;--AND f.credito_key &#x3d; 6419&#xd;&#xa;--fecha_key between &#x27;2012-03-01&#x27; and &#x27;2012-03-31&#x27;&#xd;&#xa;--ORDER BY f.fecha_key &#xd;&#xa;&#xd;&#xa;</sql>
