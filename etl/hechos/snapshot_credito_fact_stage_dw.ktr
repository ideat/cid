  <step>
    <name>Obtener clases de rango y desembolso&#x3b; bandera mora</name>
    <type>UserDefinedJavaClass</type>
    <description/>
    <distribute>Y</distribute>
    <custom_distribution/>
    <copies>1</copies>
         <partitioning>
           <method>none</method>
           <schema_name/>
           </partitioning>

    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import java.math.BigDecimal;
import java.util.Date;
import org.joda.time.DateTime;

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    row = createOutputRow(row, data.outputRowMeta.size());

	Long diasMora = get(Fields.In, "dias_mora").getInteger(row);
	Long huboMora = diasMora > 0L ? 1L : 0L;
	Double montoTotalDesembolsadoME = get(Fields.In, "monto_total_desembolsado_me").getBigNumber(row).doubleValue();
	String estadoNombre = get(Fields.In, "estado_nombre").getString(row);

	String claseMontoDesembolsoME = "DESCONOCIDO";
	Long claseMontoDesembolsoMEOrden = 0L;
	if (0.0 <= montoTotalDesembolsadoME && montoTotalDesembolsadoME <= 3000.0) {
		claseMontoDesembolsoME = "<3000";
		claseMontoDesembolsoMEOrden = 1L;
	} else if (3000.0 < montoTotalDesembolsadoME && montoTotalDesembolsadoME <= 5000.0) {
		claseMontoDesembolsoME = "3001-5000";
		claseMontoDesembolsoMEOrden = 2L;	
	} else if (5000.0 < montoTotalDesembolsadoME && montoTotalDesembolsadoME <= 20000.0) {
		claseMontoDesembolsoME = "5001-20000";
		claseMontoDesembolsoMEOrden = 3L;
	} else if (20000.0 < montoTotalDesembolsadoME && montoTotalDesembolsadoME <= 50000.0) {
		claseMontoDesembolsoME = "20001-50000";
		claseMontoDesembolsoMEOrden = 4L;
	} else if (50000.0 < montoTotalDesembolsadoME && montoTotalDesembolsadoME <= 120000.0) {
		claseMontoDesembolsoME = "50001-120000";
		claseMontoDesembolsoMEOrden = 5L;
	} else {
		claseMontoDesembolsoME = ">120000";
		claseMontoDesembolsoMEOrden = 6L;
	}	


	String claseDiasMora = "DESCONOCIDO";
	Long claseDiasMoraOrden = 0L;
	if (0 >= diasMora) {
		claseDiasMora = "0";
		claseDiasMoraOrden = 1L;
	} else if (1 <= diasMora && diasMora <= 5) {
		claseDiasMora = "1-5";
		claseDiasMoraOrden = 2L;
	} else if (6 <= diasMora && diasMora <= 30) {
		claseDiasMora = "6-30";
		claseDiasMoraOrden = 3L;
	} else if (31 <= diasMora && diasMora <= 60) {
		claseDiasMora = "31-60";
		claseDiasMoraOrden = 4L;
	} else if (61 <= diasMora && diasMora <= 90) {
		claseDiasMora = "61-90";
		claseDiasMoraOrden = 5L;
	} else if (91 <= diasMora && diasMora <= 180) {
		claseDiasMora = "91-180";
		claseDiasMoraOrden = 6L;
	} else if (181 <= diasMora && diasMora <= 365) {
		claseDiasMora = "181-365";
		claseDiasMoraOrden = 7L;
	} else {
		claseDiasMora = ">365";
		claseDiasMoraOrden = 8L;
	}


	String estadoCategoria = "NO ACTIVA";
	if (estadoNombre.startsWith("VIGENTE") || estadoNombre.startsWith("VENCIDO") || estadoNombre.startsWith("EJECUCION")) {
		estadoCategoria = "ACTIVA";
	}

	Date fechaMetaOficial = new DateTime(get(Fields.In, "fecha_snapshot").getDate(row)).dayOfMonth().withMaximumValue().toDate();
	

//	logBasic("*** diasMora *** > " + diasMora);
//	logBasic("*** claseDiasMora *** > " + claseDiasMora);

//	logBasic("*** montoTotalDesembolsadoME *** > " + montoTotalDesembolsadoME);
//	logBasic("*** claseMontoDesembolsoME *** > " + claseMontoDesembolsoME);



    get(Fields.Out, "hubo_mora").setValue(row, huboMora);	
    get(Fields.Out, "clase_por_monto_desembolso_me").setValue(row, claseMontoDesembolsoME);	
    get(Fields.Out, "clase_por_dias_mora").setValue(row, claseDiasMora);
	get(Fields.Out, "estado_categoria").setValue(row, estadoCategoria);
	get(Fields.Out, "fecha_meta_oficial").setValue(row, fechaMetaOficial);
    get(Fields.Out, "clase_por_monto_desembolso_me_orden").setValue(row, claseMontoDesembolsoMEOrden);	
    get(Fields.Out, "clase_por_dias_mora_orden").setValue(row, claseDiasMoraOrden);	
	
	

	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
]]></class_source>
