    <sql>SELECT&#xa;   fecha_snapshot,&#xd;&#xa;   CAST&#x28;to_char&#x28;fecha_snapshot, &#x27;YYYYMMDD&#x27;&#x29; AS BIGINT&#x29; as numero_fecha,&#xd;&#xa;   prmprnpre,&#xd;&#xa;   prmprcage,&#xd;&#xa;   prmprrseg,&#xd;&#xa;   prmpragen,&#xd;&#xa;   prmprcmon,&#xd;&#xa;   prmprlncr,&#xd;&#xa;   saldo,&#xd;&#xa;   saldo_me,&#xd;&#xa;   CASE &#xd;&#xa;	WHEN saldo_me &#x3e;&#x3d; 30001 THEN &#x27;&#x3e;&#x3d; 30.001&#x27; &#xd;&#xa;	WHEN saldo_me BETWEEN 20001 AND 30000 THEN &#x27;20.001-30.000&#x27; &#xd;&#xa;	WHEN saldo_me BETWEEN 15001 AND 20000 THEN &#x27;15.001-20.000&#x27; &#xd;&#xa;	WHEN saldo_me BETWEEN 10001 AND 15000 THEN &#x27;10.001-15.000&#x27; &#xd;&#xa;	WHEN saldo_me BETWEEN 6001 AND 10000 THEN &#x27;6.001-10.000&#x27; &#xd;&#xa;	WHEN saldo_me BETWEEN 3001 AND 6000 THEN &#x27;3.001-6.000&#x27; &#xd;&#xa;	WHEN saldo_me BETWEEN 1001 AND 3000 THEN &#x27;1.001-3.000&#x27; &#xd;&#xa;	ELSE &#x27;&#x3c;&#x3d;1000&#x27;&#xd;&#xa;   END as clase_por_saldo_me,&#xd;&#xa;   CASE &#xd;&#xa;	WHEN saldo_me &#x3e;&#x3d; 30001 THEN 8 &#xd;&#xa;	WHEN saldo_me BETWEEN 20001 AND 30000 THEN 7 &#xd;&#xa;	WHEN saldo_me BETWEEN 15001 AND 20000 THEN 6 &#xd;&#xa;	WHEN saldo_me BETWEEN 10001 AND 15000 THEN 5&#xd;&#xa;	WHEN saldo_me BETWEEN 6001 AND 10000 THEN 4 &#xd;&#xa;	WHEN saldo_me BETWEEN 3001 AND 6000 THEN 3 &#xd;&#xa;	WHEN saldo_me BETWEEN 1001 AND 3000 THEN 2 &#xd;&#xa;	ELSE 1&#xd;&#xa;   END as clase_por_saldo_me_orden,&#xd;&#xa;   saldo_me &#x2a; &#x28;tasa_variable&#x2f;100&#x29; as saldo_ponderado,&#xd;&#xa;   hubo_desembolso,&#xd;&#xa;   monto_desembolsado,&#xd;&#xa;   monto_desembolsado_me,&#xd;&#xa;   hubo_recuperacion,&#xd;&#xa;   monto_recuperado,&#xd;&#xa;   monto_recuperado_me,&#xd;&#xa;   hubo_intereses,&#xd;&#xa;   monto_intereses,&#xd;&#xa;   monto_intereses_me,&#xd;&#xa;   tasa_variable,&#xd;&#xa;   es_cliente_nuevo,&#xd;&#xa;   codigo_cliente,&#xd;&#xa;   codigo_oficial,&#xd;&#xa;   moneda,&#xd;&#xa;   tipo_cambio_me,&#xd;&#xa;   monto_total_desembolsado,&#xd;&#xa;   monto_total_desembolsado_me,&#xd;&#xa;   dias_mora,&#xd;&#xa;   hubo_castigo,&#xd;&#xa;   saldo_castigado,&#xd;&#xa;   saldo_castigado_me,&#xd;&#xa;   hubo_cancelacion,&#xd;&#xa;   estado,&#xd;&#xa;   esta_adelantado,&#xa;  total_monto_recuperado,&#xa;  total_monto_recuperado_me,&#xa;  total_monto_recuperado_plan_pagos,&#xa;  total_monto_recuperado_plan_pagos_me,&#xd;&#xa;  diferencia_monto_recuperado_plan_pago,&#xd;&#xa;  diferencia_monto_recuperado_plan_pago_me,&#xd;&#xa;  monto_cuota_capital_siguiente_mes,&#xd;&#xa;  monto_cuota_capital_siguiente_mes_me,&#xd;&#xa;  monto_prevision_mn,&#xd;&#xa;  monto_prevision_me,&#xd;&#xa;  monto_prevision_fin_mes_estimado_mn,&#xd;&#xa;  monto_prevision_fin_mes_estimado_me,&#xd;&#xa;  dias_hasta_fin_plan_pago,&#xd;&#xa;   CASE &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago &#x3e; 180 THEN &#x27;&#x3e; 180&#x27; &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 91 AND 180 THEN &#x27;91-180&#x27; &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 61 AND 90 THEN &#x27;61-90&#x27; &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 31 AND 60 THEN &#x27;31-60&#x27;&#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 1 AND 30 THEN &#x27;1-30&#x27; &#xd;&#xa;	ELSE &#x27;&#x3c;&#x3d;0&#x27;&#xd;&#xa;   END as clase_por_dias_hasta_fin_plan_pago,&#xd;&#xa;   CASE &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago &#x3e; 180 THEN 6 &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 91 AND 180 THEN 5 &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 61 AND 90 THEN 4 &#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 31 AND 60 THEN 3&#xd;&#xa;	WHEN dias_hasta_fin_plan_pago BETWEEN 1 AND 30 THEN 2 &#xd;&#xa;	ELSE 1&#xd;&#xa;   END as clase_por_dias_hasta_fin_plan_pago_orden&#xa;FROM snapshot_credito_fact_tra&#xa;--LIMIT 10 OFFSET 5090&#xa;</sql>
