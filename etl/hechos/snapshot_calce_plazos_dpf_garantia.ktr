    <sql>select to_date&#x28;&#x27;&#x24;&#x7b;fecha_corte&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29; as fecha_corte, &#x27;OBLIGACIONES RESTRINGIDAS&#x27; as grupo, &#x27;PASIVO&#x27; as tipo, &#xd;&#xa;&#x27;DPF AFECTADO EN GARANTIA&#x27; as concepto, 1030 as correlativo, &#xd;&#xa;sum&#x28;&#xd;&#xa;CASE&#xd;&#xa;  when m.codigo_tx &#x3d; 1 then t.monto_original&#xd;&#xa;  else 0&#xd;&#xa;end&#x29; as monto_30_bs,&#xd;&#xa;&#xd;&#xa;sum&#x28;&#xd;&#xa;CASE&#xd;&#xa;  when m.codigo_tx &#x3d; 2 then t.monto_original &#x2a; to_number&#x28;&#x27;&#x24;&#x7b;tipo_cambio&#x7d;&#x27;, &#x27;9.99&#x27;&#x29;&#xd;&#xa;  else 0&#xd;&#xa;end&#x29; as monto_30_sus,&#xd;&#xa;&#xd;&#xa;sum&#x28;&#xd;&#xa;CASE&#xd;&#xa;  when m.codigo_tx &#x3d; 1 then t.monto_original&#xd;&#xa;  when m.codigo_tx &#x3d; 2 then t.monto_original &#x2a; to_number&#x28;&#x27;&#x24;&#x7b;tipo_cambio&#x7d;&#x27;, &#x27;9.99&#x27;&#x29;&#xd;&#xa;  else 0&#xd;&#xa;end&#x29; as monto_30&#xd;&#xa;&#xd;&#xa;from tipo_garantia_dim d&#xd;&#xa;inner join transaccion_garantia_fact t on &#x28;d.tipo_garantia_key &#x3d; t.tipo_garantia_key&#x29;&#xd;&#xa;inner join credito_dim c on &#x28;c.credito_key &#x3d; t.credito_key&#x29;&#xd;&#xa;inner join moneda_dim m on &#x28;t.moneda_key &#x3d; m.moneda_key&#x29;&#xd;&#xa;where&#xd;&#xa;d.abreviacion &#x3d; &#x27;BM2&#x27;&#xd;&#xa;and c.es_actual &#x3d; &#x27;Y&#x27;&#xd;&#xa;and c.fecha_vencimiento_final &#x3c;&#x3d; to_date&#x28;&#x27;&#x24;&#x7b;fecha_corte&#x7d;&#x27;,&#x27;yyyy-MM-dd&#x27;&#x29;&#x2b;cast&#x28;&#x27;30 days&#x27; as interval&#x29;&#xd;&#xa;and c.estado not in &#x28;&#x27;CANCELADO&#x27;,&#x27;CASTIGADO&#x27;,&#x27;DESCONOCIDO&#x27;&#x29;&#xd;&#xa;and t.agencia_key &#x3d; &#x3f;</sql>
    <outer_join>N</outer_join>
