    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first)
    {
        first = false;
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    row = createOutputRow(row, data.outputRowMeta.size());


	//Estado Civil
	java.util.Date fechaDeclaracion = get(Fields.In, "fecha_declaracion").getDate(row);
	java.util.Date fechaFiliacion = get(Fields.In, "fecha_filiacion").getDate(row);
	if (fechaDeclaracion == null) {
		fechaDeclaracion = fechaFiliacion;
	}

    get(Fields.Out, "fecha_declaracion").setValue(row, fechaDeclaracion);
	
	
 
	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
]]></class_source>
        </definition>
