    <definitions>
        <definition>
        <class_type>TRANSFORM_CLASS</class_type>

        <class_name>Processor</class_name>

        <class_source><![CDATA[import org.joda.time.MutableDateTime;

private java.text.SimpleDateFormat dateFormatter;

private MutableDateTime finPrimerTrimestre = new MutableDateTime(2000, 3, 31, 0, 0, 0, 0);
private MutableDateTime finSegundoTrimestre = new MutableDateTime(2000, 6, 30, 0, 0, 0, 0);
private MutableDateTime finTercerTrimestre = new MutableDateTime(2000, 9, 30, 0, 0, 0, 0);
private MutableDateTime finCuartoTrimestre = new MutableDateTime(2000, 12, 31, 0, 0, 0, 0);
private MutableDateTime today = new MutableDateTime(2000, 12, 31, 0, 0, 0, 0);


private String getIsUltimoDiaMes(MutableDateTime today) {
	boolean result = false;
	switch (today.getMonthOfYear()) {
            case 1:  
            case 3:  
            case 5:  
            case 7:  
            case 8:  
            case 10:  
            case 12:  
					result = (today.getDayOfMonth() == 31);
					break;

            case 4:  
            case 6:  
            case 9:  
            case 11:
					result = (today.getDayOfMonth() == 30);
					break;  

            case 2:  
					result = today.year().isLeap() ? today.getDayOfMonth() == 29 : today.getDayOfMonth() == 28; 
					break;	
			default:
					break;
        }	

	return ((result) ? "Y" : "N");
}

private String getIsUltimoDiaTrimestre(MutableDateTime today){
	boolean result = finPrimerTrimestre.equals(today) 
					  || finSegundoTrimestre.equals(today) 
   				      || finTercerTrimestre.equals(today)
   				      || finCuartoTrimestre.equals(today);

	return ((result) ? "Y" : "N");
}

private String getIsUltimoDiaSemestre(MutableDateTime today){
	boolean result = finSegundoTrimestre.equals(today) 
   				      || finCuartoTrimestre.equals(today);
	return ((result) ? "Y" : "N");
}

private String getIsUltimoDiaAnio(MutableDateTime today){
	boolean result = finCuartoTrimestre.equals(today);
	return ((result) ? "Y" : "N");
}

public boolean processRow(StepMetaInterface smi, StepDataInterface sdi) throws KettleException
{
	Object[] row = getRow();
	if (row == null) {
		setOutputDone();
		return false;
	}

    if (first) {
        first = false;
		dateFormatter = new java.text.SimpleDateFormat();
    }

	// It is always safest to call createOutputRow() to ensure that your output row's Object[] is large
    // enough to handle any new fields you are creating in this step.
    row = createOutputRow(row, data.outputRowMeta.size());

	java.util.Date fecha = get(Fields.In, "fecha_key").getDate(row);
    finPrimerTrimestre.setYear( get(Fields.In, "anio").getInteger(row).intValue() );
    finSegundoTrimestre.setYear( get(Fields.In, "anio").getInteger(row).intValue() );    
    finTercerTrimestre.setYear( get(Fields.In, "anio").getInteger(row).intValue() );
    finCuartoTrimestre.setYear( get(Fields.In, "anio").getInteger(row).intValue() );


    /*Dias*/
	long diaTrimestre = get(Fields.In, "dia_anio").getInteger(row);
	if ( diaTrimestre > finPrimerTrimestre.getDayOfYear() && diaTrimestre <= finSegundoTrimestre.getDayOfYear() ) { //2 semestre
		diaTrimestre = diaTrimestre - finPrimerTrimestre.getDayOfYear();
	} else if ( diaTrimestre > finSegundoTrimestre.getDayOfYear() && diaTrimestre <= finTercerTrimestre.getDayOfYear()) { //3 semestre
		diaTrimestre = diaTrimestre - finSegundoTrimestre.getDayOfYear();
	} else if (diaTrimestre > finTercerTrimestre.getDayOfYear() && diaTrimestre <= finCuartoTrimestre.getDayOfYear()) { //4 semestre
		diaTrimestre = diaTrimestre - finTercerTrimestre.getDayOfYear();
	}


	long diaSemestre = get(Fields.In, "dia_anio").getInteger(row);
	if (diaSemestre > finSegundoTrimestre.getDayOfYear() ) { 	//2 semestre
		diaSemestre = diaSemestre - finSegundoTrimestre.getDayOfYear();
	} 

	/*Semanas*/
	dateFormatter.applyPattern("W");
	long semanaMes = Long.parseLong(dateFormatter.format(fecha));

	
	long semanaTrimestre = get(Fields.In, "dia_anio").getInteger(row);
	long semanaAnio = get(Fields.In, "semana_anio").getInteger(row);
	
	//Arreglar bug en el calculo de la ultima semana del aÃ±o, que puede ser la 53
	if (semanaAnio == 1 && get(Fields.In, "mes_anio").getInteger(row)==12) {
		semanaAnio = 53;
		get(Fields.Out, "semana_anio").setValue(row, semanaAnio);
	}

	if (semanaTrimestre > finPrimerTrimestre.getDayOfYear() && semanaTrimestre <= finSegundoTrimestre.getDayOfYear() ) { //2 trimestre
		semanaTrimestre = semanaAnio - finPrimerTrimestre.getWeekOfWeekyear();
	} else if (semanaTrimestre > finSegundoTrimestre.getDayOfYear() && semanaTrimestre <= finTercerTrimestre.getDayOfYear()) { //3 trimestre
		semanaTrimestre = semanaAnio - finSegundoTrimestre.getWeekOfWeekyear();
	} else if (semanaTrimestre > finTercerTrimestre.getDayOfYear() && semanaTrimestre <= finCuartoTrimestre.getDayOfYear()) { //4 trimestre
		semanaTrimestre = semanaAnio - finTercerTrimestre.getWeekOfWeekyear();
	} else {
		semanaTrimestre = semanaAnio;
	}
	if (semanaTrimestre == 0) semanaTrimestre++;

	long semanaSemestre = get(Fields.In, "dia_anio").getInteger(row);
	if (semanaSemestre > finSegundoTrimestre.getDayOfYear() ) {
		semanaSemestre = semanaAnio - finSegundoTrimestre.getWeekOfWeekyear();
	} else {
		semanaSemestre = semanaAnio;
	}
	if (semanaSemestre == 0) semanaSemestre++;

	/*Meses*/
	long mesTrimestre = get(Fields.In, "dia_anio").getInteger(row);
	if (mesTrimestre > finPrimerTrimestre.getDayOfYear() && mesTrimestre <= finSegundoTrimestre.getDayOfYear() ) { //2 trimestre
		mesTrimestre = get(Fields.In, "mes_anio").getInteger(row) - finPrimerTrimestre.getMonthOfYear();
	} else if (mesTrimestre > finSegundoTrimestre.getDayOfYear() && mesTrimestre <= finTercerTrimestre.getDayOfYear()) { //3 trimestre
		mesTrimestre = get(Fields.In, "mes_anio").getInteger(row) - finSegundoTrimestre.getMonthOfYear();
	} else if (mesTrimestre > finTercerTrimestre.getDayOfYear() && mesTrimestre <= finCuartoTrimestre.getDayOfYear()) { //4 trimestre
		mesTrimestre = get(Fields.In, "mes_anio").getInteger(row) - finTercerTrimestre.getMonthOfYear();
	} else {
		mesTrimestre = get(Fields.In, "mes_anio").getInteger(row);
	}	
	if (mesTrimestre == 0) mesTrimestre++;

	long mesSemestre = get(Fields.In, "dia_anio").getInteger(row);
	if (mesSemestre  > finSegundoTrimestre.getDayOfYear() ) {
		mesSemestre  = get(Fields.In, "mes_anio").getInteger(row) - finSegundoTrimestre.getMonthOfYear();
	} else {
		mesSemestre  = get(Fields.In, "mes_anio").getInteger(row);
	}
	if (mesSemestre  == 0) mesSemestre ++;

	long mesDecada = get(Fields.In, "mes_anio").getInteger(row) + ((get(Fields.In, "anio").getInteger(row) % 10) * 12);


	/*Trimestre*/
	long trimestreSemestre = get(Fields.In, "dia_anio").getInteger(row);
	if (trimestreSemestre > finPrimerTrimestre.getDayOfYear() && trimestreSemestre <= finSegundoTrimestre.getDayOfYear() ) { //2 trimestre
		trimestreSemestre = 2;
	} else if (trimestreSemestre > finSegundoTrimestre.getDayOfYear() && trimestreSemestre <= finTercerTrimestre.getDayOfYear()) { //3 trimestre
		trimestreSemestre = 1;
	} else if (trimestreSemestre > finTercerTrimestre.getDayOfYear() && trimestreSemestre <= finCuartoTrimestre.getDayOfYear()) { //4 trimestre
		trimestreSemestre = 2;
	} else {
		trimestreSemestre = 1; //1er trimestre
	}

	long trimestreAnio = get(Fields.In, "dia_anio").getInteger(row);
	if (trimestreAnio > finPrimerTrimestre.getDayOfYear() && trimestreAnio <= finSegundoTrimestre.getDayOfYear() ) { //2 trimestre
		trimestreAnio = 2;
	} else if (trimestreAnio > finSegundoTrimestre.getDayOfYear() && trimestreAnio <= finTercerTrimestre.getDayOfYear()) { //3 trimestre
		trimestreAnio = 3;
	} else if (trimestreAnio > finTercerTrimestre.getDayOfYear() && trimestreAnio <= finCuartoTrimestre.getDayOfYear()) { //4 trimestre
		trimestreAnio = 4;
	} else {
		trimestreAnio = 1; //1er trimestre
	}

	/*semestre*/
	long semestreAnio = get(Fields.In, "dia_anio").getInteger(row);
	if (semestreAnio > finSegundoTrimestre.getDayOfYear() ) { //2 trimestre
		semestreAnio = 2;
	} else {
		semestreAnio = 1; //1er trimestre
	}

   /*decada*/
	long decada = get(Fields.In, "anio").getInteger(row);
    decada = (decada / 100) * 100;


    get(Fields.Out, "dia_trimestre").setValue(row, diaTrimestre);
    get(Fields.Out, "dia_semestre").setValue(row, diaSemestre);

    get(Fields.Out, "semana_mes").setValue(row, semanaMes);
    get(Fields.Out, "semana_trimestre").setValue(row, semanaTrimestre);
    get(Fields.Out, "semana_semestre").setValue(row, semanaSemestre);

    get(Fields.Out, "mes_trimestre").setValue(row, mesTrimestre);
    get(Fields.Out, "mes_semestre").setValue(row, mesSemestre);
    get(Fields.Out, "mes_decada").setValue(row, mesDecada);

    get(Fields.Out, "trimestre_semestre").setValue(row, trimestreSemestre);
    get(Fields.Out, "trimestre_anio").setValue(row, trimestreAnio);
    get(Fields.Out, "semestre_anio").setValue(row, semestreAnio);
    get(Fields.Out, "decada").setValue(row, decada);

	/*Banderas*/
	today.setYear(get(Fields.In, "anio").getInteger(row).intValue());
	today.setMonthOfYear(get(Fields.In, "mes_anio").getInteger(row).intValue());
	today.setDayOfMonth(get(Fields.In, "dia_mes").getInteger(row).intValue()); 

    get(Fields.Out, "ult_dia_mes").setValue(row, getIsUltimoDiaMes(today ));
    get(Fields.Out, "ult_dia_trimestre").setValue(row, getIsUltimoDiaTrimestre( today));
    get(Fields.Out, "ult_dia_semestre").setValue(row, getIsUltimoDiaSemestre(today ));
    get(Fields.Out, "ult_dia_anio").setValue(row, getIsUltimoDiaAnio(today ));

	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
