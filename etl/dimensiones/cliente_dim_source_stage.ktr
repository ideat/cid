    row = createOutputRow(row, data.outputRowMeta.size());
	
	String nombre = get(Fields.In, "gbagenomb").getString(row);
	nombre = nombre.trim().toUpperCase();
    get(Fields.Out, "gbagenomb").setValue(row, nombre);


	//Nacionalidad 
	String nacionalidad = get(Fields.In, "gbagenaci").getString(row);
	nacionalidad = nacionalidad .trim().toUpperCase();
	if (nacionalidad.length() <= 2 ) {
		nacionalidad = "DESCONOCIDA";	
	}
	nacionalidad = nacionalidad.substring(0, nacionalidad.length() - 1) + "A";
    get(Fields.Out, "gbagenaci").setValue(row, nacionalidad);

	//Genero
	String genero = "DESCONOCIDO";
	long codigoGenero = get(Fields.In, "gbagesexo").getInteger(row);
	switch ((int) codigoGenero) {
		case 1: genero = "MASCULINO";
				break;
		case 2: genero = "FEMENINO";
				break;

	}
    get(Fields.Out, "gbagesexo_str").setValue(row, genero);

	//Estado Civil
	String estadoCivil = "DESCONOCIDO";
	long codigoEstadoCivil = get(Fields.In, "gbageeciv").getInteger(row);
	switch ((int) codigoEstadoCivil) {
		case 1: estadoCivil = "SOLTERO";
				break;
		case 2: estadoCivil = "CASADO";
				break;
		case 3: estadoCivil = "DIVORCIADO";
				break;
		case 4: estadoCivil = "VIUDO";
				break;

	}
    get(Fields.Out, "gbageeciv_str").setValue(row, estadoCivil);
	
    
    String tamanio = "DESCONOCIDO";
	long codigoTamanio = get(Fields.In, "gbdbitpre").getInteger(row);
	switch ((int) codigoTamanio) {
		case 1: tamanio = "MICRO EMPRESA";
				break;
		case 2: tamanio = "PEQUEÃ‘A EMPRESA";
				break;
		case 3: tamanio = "MEDIANA EMPRESA";
				break;
		case 4: tamanio = "GRANDE EMPRESA";
				break;

	}
    get(Fields.Out, "gbdbitpre_str").setValue(row, tamanio);

	// Send the row on to the next step.
    putRow(data.outputRowMeta, row);

	return true;
}
