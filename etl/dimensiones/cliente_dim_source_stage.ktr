    <sql>SELECT  &#xa;	   gbagecage -- codigo_tx&#xa;	  ,case&#xd;&#xa;        when gbagenomb is null then &#x27;DESCONOCIDO&#x27; -- nombre&#xd;&#xa;        else gbagenomb&#xd;&#xa;       end as gbagenomb  &#xa;	  ,case&#xd;&#xa;        when gbagesexo is null then 0 -- genero &#x28;int&#x29; &#xd;&#xa;        else gbagesexo&#xd;&#xa;       end as gbagesexo&#xa;	  ,case &#xd;&#xa;        when gbagefnac is null then date&#x28;&#x27;1900-01-01&#x27;&#x29; -- fecha_nac&#xd;&#xa;        else gbagefnac&#xd;&#xa;       end as gbagefnac&#xa;	  ,case &#xd;&#xa;        when gbagefreg is null then date&#x28;&#x27;1900-01-01&#x27;&#x29; -- fecha_filiacion&#xd;&#xa;        else gbagefreg&#xd;&#xa;       end as gbagefreg         &#xa;	  ,case &#xd;&#xa;         when gbageeciv is null then 0 -- estado_civil &#x28;int&#x29;&#xd;&#xa;         else gbageeciv&#xd;&#xa;       end as gbageeciv    &#xa;	  ,case &#xd;&#xa;        when gbagenaci is null then &#x27;DESCONOCIDO&#x27;-- nacionalidad&#xd;&#xa;        else gbagenaci&#xd;&#xa;       end as gbagenaci,&#xd;&#xa;       gbageciiu,&#xd;&#xa;       gbagecalf,&#xd;&#xa;       trim&#x28;c.gbcondesc&#x29; as gbagetper, --Tipo de persona&#xd;&#xa;       trim&#x28;d.gbcondesc&#x29; as gbdacnive, -- Nivel estudio&#xd;&#xa;       gbdaccand , --Numero dependientes&#xd;&#xa;       trim&#x28;e.gbcondesc&#x29; as gbdactres, -- Tipo vivienda&#xd;&#xa;	   a.gbdbiaemp , --Antiguedad empresa&#xd;&#xa;       trim&#x28;f.gbcondesc&#x29; as gbdactipo, -- Tipo cliente&#xd;&#xa;       a.gbdbicper,  -- Personal ocupado &#xd;&#xa;       0 as cantidad_creditos,&#xd;&#xa;       &#xd;&#xa;        case &#xd;&#xa;         when gbdbifech is null then date&#x28;&#x27;1900-01-01&#x27;&#x29;   --fecha de declaracion&#xd;&#xa;         else gbdbifech&#xd;&#xa;       end as gbdbifech&#xd;&#xa;	  ,case&#xd;&#xa;         when gbdbiifij is null then 0   -- Ingresos Fijos&#xd;&#xa;         else gbdbiifij&#xd;&#xa;       end as gbdbiifij &#xd;&#xa;	  ,case &#xd;&#xa;         when gbdbiivar is null then 0  -- Ingresos Variables&#xd;&#xa;         else gbdbiivar&#xd;&#xa;       end as gbdbiivar&#xd;&#xa;	  ,case &#xd;&#xa;         when gbdbigfij is null then 0  -- Gastos Fijos&#xd;&#xa;         else gbdbigfij&#xd;&#xa;       end as gbdbigfij  &#xd;&#xa;	  ,case &#xd;&#xa;         when gbdbigvar is null then 0  -- Gastos Variables&#xd;&#xa;         else gbdbigvar&#xd;&#xa;       end as gbdbigvar &#xd;&#xa;	  ,case &#xd;&#xa;         when gbdbitact is null then 0  -- Total Activos&#xd;&#xa;         else gbdbitact&#xd;&#xa;       end as gbdbitact&#xd;&#xa;	  ,case &#xd;&#xa;         when gbdbitpas is null then 0  -- Total Pasivos&#xd;&#xa;         else gbdbitpas&#xd;&#xa;       end as gbdbitpas&#xd;&#xa;	  ,case &#xd;&#xa;		 when gbdbipatr is null then 0  -- Patrimonio&#xd;&#xa;         else gbdbipatr&#xd;&#xa;       end as gbdbipatr &#xd;&#xa;	  ,case &#xd;&#xa;         when gbdbitpre is null then 0  -- Tamano Cliente&#xd;&#xa;         else gbdbitpre&#xd;&#xa;       end as gbdbitpre&#xd;&#xa;      ,gbageagen &#xd;&#xa;      ,case &#xd;&#xa;         when gbageddo1 is null then &#x27;&#x27;  -- Direccion oficina&#xd;&#xa;         else gbageddo1&#xd;&#xa;       end as gbageddo1&#xd;&#xa;	  ,case &#xd;&#xa;         when gbageddo2 is null then &#x27;&#x27;  -- Direccion oficina&#xd;&#xa;         else gbageddo2&#xd;&#xa;       end as gbageddo2&#xd;&#xa;	  ,case &#xd;&#xa;         when gbagedir1 is null then &#x27;&#x27;  -- Direccion domicilio&#xd;&#xa;         else gbagedir1&#xd;&#xa;       end as gbagedir1&#xd;&#xa;      ,case &#xd;&#xa;         when gbagedir2 is null then &#x27;&#x27;  -- Direccion domicilio&#xd;&#xa;         else gbagedir2&#xd;&#xa;       end as gbagedir2&#xd;&#xa;      ,case &#xd;&#xa;         when gbagetlfd is null then &#x27;&#x27;  -- Telefono domicilio&#xd;&#xa;         else gbagetlfd&#xd;&#xa;       end as gbagetlfd&#xd;&#xa;      ,case &#xd;&#xa;         when gbagetlfo is null then &#x27;&#x27;  -- Telefono oficina&#xd;&#xa;         else gbagetlfo&#xd;&#xa;       end as gbagetlfo&#xd;&#xa;&#xd;&#xa;    &#xa;FROM gbage&#xd;&#xa;left join gbdac on &#x28;gbagecage &#x3d; gbdaccage&#x29;&#xd;&#xa;left join gbcon c on &#x28;c.gbconpfij &#x3d; 1 and c.gbconcorr &#x3d; gbagetper&#x29;&#xd;&#xa;left join gbcon d on &#x28;d.gbconpfij &#x3d; 74 and d.gbconcorr &#x3d; gbdacnive&#x29;&#xd;&#xa;left join gbcon e on &#x28;e.gbconpfij &#x3d; 65 and e.gbconcorr &#x3d; gbdactres&#x29;&#xd;&#xa;left join gbcon f on &#x28;f.gbconpfij &#x3d; 43 and f.gbconcorr &#x3d; gbdactipo&#x29;&#xd;&#xa;&#xd;&#xa;left join gbdbi a on &#x28;a.gbdbicage &#x3d; gbagecage &#xd;&#xa;and a.gbdbifech &#x3d;  &#x28;select max&#x28;b.gbdbifech&#x29;&#xd;&#xa;            from gbdbi b&#xd;&#xa;            where b.gbdbicage &#x3d; a.gbdbicage&#xd;&#xa;            &#x29;&#x29;&#xd;&#xa;--WHERE gbagemrcb &#x3d; 0&#xd;&#xa;order by gbagecage&#xd;&#xa; </sql>
